{
  "name": "autohand-cli",
  "version": "0.1.0",
  "description": "Autohand interactive coding agent CLI powered by LLMs.",
  "type": "module",
  "bin": {
    "autohand": "dist/index.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "start": "node dist/index.js"
  },
  "keywords": [
    "cli",
    "llm",
    "agent",
    "autohand"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "diff": "^8.0.2",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.3.2",
    "ignore": "^5.3.1",
    "ink": "^4.4.1",
    "ora": "^9.0.0",
    "react": "^18.2.0",
    "terminal-link": "^3.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/diff": "^8.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.3",
    "@types/terminal-link": "^1.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^1.6.0"
  }
}
